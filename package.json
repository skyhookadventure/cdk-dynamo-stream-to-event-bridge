{
  "name": "cdk-dynamo-stream-to-event-bridge",
  "license": "MIT",
  "version": "2.0.2",
  "main": "dist/index.js",
  "description": "Stream events from Dynamo DB to EventBridge.",
  "scripts": {
    "build": "tsc --resolveJsonModule",
    "watch": "tsc -w --resolveJsonModule",
    "test": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/jest --collect-coverage",
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "bump": "./node_modules/.bin/versiony package.json --patch"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.68.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "aws-cdk-lib": "^2.138.0",
    "aws-sdk": "^2.1603.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.20.2",
    "eslint": "^9.1.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-2": "npm:prettier@^2",
    "ts-jest": "^29.1.2",
    "typescript": "~5.4.5",
    "versiony-cli": "^1.3.0"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "prettierPath": "prettier-2",
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleNameMapper": {
      "@aws-cdk/aws-lambda-nodejs": "cdk-lambda-nodejs-mock",
      "aws-cdk-lib/aws-lambda-nodejs": "cdk-lambda-nodejs-mock"
    }
  },
  "peerDependencies": {
    "@aws-cdk/assert": "^2",
    "aws-cdk-lib": "^2",
    "constructs": "^10"
  },
  "dependencies": {
    "cdk-lambda-nodejs-mock": "^2.0.2"
  },
  "packageManager": "yarn@4.1.1"
}
